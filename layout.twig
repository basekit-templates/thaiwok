<!doctype html>
<html lang="{{ page.seoLang }}">

	<head>
	
		{% include "head.twig" %}
		
		{% include basekit.headScript %}

	</head>

	<body class="{{ page.backgroundClass }}">

<header class="bk-header">
			{{widget('companyname', 'companyname', {"business": "profile"})|raw}}
		</header>

		<nav class="nav">
			{{widget('extendednavigation', 'navigationheader', {})|raw}}
		</nav>

		<div class="page-wrap">
		
			{% include "header.twig" %}

			{% block content %}
			{% endblock %}

			{% include "footer.twig" %}

		</div>

		{% include basekit.bodyScript %}

		<script type="text/javascript">
			!function() {
				function supportsSVG() {
					return !!document.createElementNS && !! document.createElementNS('http://www.w3.org/2000/svg', 'svg').createSVGRect
				}
				if (supportsSVG()) document.documentElement.className += ' svg'
			}()
		</script>

	</body>
</html>
